/*Faça um programa que leia cinco números inteiros, calcule e escreva
a soma o produto e a média dos números lidos */
//Inclusão de bibliotecas
#include <stdio.h>
#include <math.h>

//Função principal
int main()
{

//Declaração de variáveis
int X, Y, result;

//Entrada de dados
    printf("Digite o valor de X\n");
    scanf("%d", &X);
    printf("Digite o valor de Y\n");
    scanf("%d", &Y);
    
    //Cálculo
    result = pow(X, Y);

//Saída de dados
printf("Valor = %d\n", result);

//Fim da função principal
return 0;
}
